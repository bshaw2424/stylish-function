<% include ../../partials/header %>
<section>
  <div class="index-container">
    <% include ../../admin/admin-nav %>
    <div class="container">
      <div class="article-container">
        <div class="row">
          <h1>welcome to product page i.e. example page</h1>
          <hr />
          <div class="col sm12 m6 l4">
            <% for(let articles of article.products){ %>
            <h4><%= articles.title %></h4>
            <div class="card">
              <div class="card-image">
                <img src="<%= articles.image %>" alt="<%= articles.image %>" />
              </div>
              <div class="card-content">
                <span>$<%= articles.price %></span>
                <p><%= articles.description %></p>
              </div>
            </div>
            <% } %>
          </div>
        </div>
        <a href="/admin.stylishfunction.com/articles" class="btn btn-small"
          >Back</a
        >
        <a href="/" class="btn">Edit</a>
        <a href="/" class="btn btn-small">delete</a>
      </div>
    </div>
  </div>
</section>
