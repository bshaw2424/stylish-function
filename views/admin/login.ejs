<% include ../partials/header %>
<article class="admin-login">
  <section class="admin-login-form container">
    <h1 class="admin-login-form-title">Admin Login</h1>
    <form action="/admin/login" method="POST" z-depth-1">
      <label for="username">Enter Username</label>
      <input type="text" id="username" name="username" />
      <label for="password">Enter Password</label>
      <input type="password" id="password" name="name" />
      <button type="submit" class="btn go-button">Login</button>
    </form>
  </section>
</article>
