<section class="featured-section">
  <div class="container">
    <div class="row">
      <h3>Featured</h3>
      <% Articles.forEach( Article => { %>
      <div class="col s12 m4 l4">
        <div class="card">
          <div class="card-image">
            <img src="<%= Article.main_image %>" alt="#">
          </div>
          <div class="card-content">
            <p><%= Article.description.brief %></p>
          </div>
        </div>
      </div>
      <% }) %>
    </div>
  </div>
</section>