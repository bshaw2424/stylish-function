<% include ../../partials/header %> <% include ../../admin/mobile-nav %>
<section class="image-update">
  <div class="image-update_container container">
    <h1>Edit Photo - <%= articles.title %></h1>
    <div class="image-container">
      <img src="<%= articles.image.url %>" alt="" />
    </div>
    <form
      action="/admin/articles/<%= articles.slug %>?_method=PUT"
      action="POST"
      enctype="multipart/form-data"
    >
      <div class="photo-update-container">
        <input type="file" />
      </div>
      <div class="button-container">
        <button type="submit" class="go-button btn">update</button>
        <a href="/admin/articles" class="static-button btn">cancel</a>
      </div>
    </form>
  </div>
</section>

<% include ../../partials/footer %>
